{
	"server_name":"n_route",
	"server_version":"v1.0",
	"server_desc":"传感类路由节点V1.0版本",
	"local_ip":"127.0.0.1",
	"local_port":40001,
	"sequence_max":100000,
	"discrete_max":100,
	"domain":"",
	"driver":
	[
		{
			"driver_name":"driver_zmq_sub",
			"lib_name": "driver_zmq.py",
			"lib_type":"python",
			"basic_config":"basic_zmq_sub.json",
			"run_config":"run_zmq_sub.json",
			"direction":"down",
			"sub_type":"seq_sensor",
			"pub_type":"dis_sensor"
		},{
			"driver_name":"driver_zmq_pub",
			"lib_name": "driver_zmq.py",
			"lib_type":"python",
			"basic_config":"basic_zmq_pub.json",
			"run_config":"run_zmq_pub.json",
			"direction":"up",
			"sub_type":"dis_sensor",
			"pub_type":"seq_sensor"
		},{
			"driver_name":"driver_websocket",
			"lib_name": "driver_websocket.py",
			"lib_type":"python",
			"basic_config":"basic_websocket.json",
			"run_config":null,
			"direction":"up",
			"sub_type":null,
			"pub_type":"seq_sensor"
		}
	],
	"topic":
	[
		{
			"topic":"all",
			"tags":""
		},{
			"topic":"control",
			"tags":""
		},{
			"topic":"websocket",
			"tags":"tag1,tag2"
		}
	],
	"pump":
	[
		{
			"topic":"all",
			"driver":"driver_zmq_pub",
			"cycle":5
		},{
			"topic":"websocket",
			"driver":"driver_websocket",
			"cycle":2
		},{
			"topic":"control",
			"driver":"driver_zmq_sub",
			"cycle":0
		}
	]
}